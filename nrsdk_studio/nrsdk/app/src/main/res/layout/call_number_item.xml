<?xml version="1.0" encoding="utf-8"?>
<com.travelrely.app.view.CallNumberItem xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/dial_num_btn"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:weightSum="3" >

    <TextView
        android:id="@+id/num"
        style="@style/style_num"
        android:layout_width="wrap_content"
        android:layout_height="0dip"
        android:layout_weight="2"
        android:gravity="center"
        android:text="1" />

    <TextView
        android:id="@+id/zimu"
        style="@style/style_num_min"
        android:layout_width="wrap_content"
        android:layout_height="0dip"
        android:layout_marginTop="@dimen/margin_small_shift"
        android:layout_weight="1"
        android:text="abc" />

</com.travelrely.app.view.CallNumberItem>