<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

			    <item  android:state_pressed="true">       
			<shape >
			
			    <solid android:color="@color/home_lable_color_2_focus" />
			
			    <corners
			        android:bottomLeftRadius="@dimen/corners_radius"
			        android:bottomRightRadius="@dimen/corners_radius"
			        android:topLeftRadius="@dimen/corners_radius"
			        android:topRightRadius="@dimen/corners_radius" >
			    </corners>
			
			    <padding
			        android:bottom="@dimen/corners_padding"
			        android:left="@dimen/corners_padding"
			        android:right="@dimen/corners_padding"
			        android:top="@dimen/corners_padding" />
			
			</shape>
			</item>
			
			    
